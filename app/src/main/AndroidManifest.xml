<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="kotako.java.info.bluebird">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="kotako.java.info.bluebird.presentation.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="kotako.java.info.bluebird.presentation.activity.TimelineActivity">
        </activity>
        <activity android:name=".presentation.activity.ProfileActivity"/>
        <activity android:name=".presentation.activity.PostActivity"/>
        <activity android:name=".presentation.activity.SettingsActivity"/>
    </application>

</manifest>